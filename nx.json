{
  "targetDefaults": {
    "test": {
      "dependsOn": ["build", "^test"],
      "cache": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/lib,dist"],
      "cache": true
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "prettier": {
      "dependsOn": ["^prettier"]
    },
    "mutate": {
      "dependsOn": ["^mutate", "test"],
      
      "cache": true
    }
  },
  "defaultBase": "main",
  "$schema": "./node_modules/nx/schemas/nx-schema.json"
}
